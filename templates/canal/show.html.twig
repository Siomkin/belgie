<!DOCTYPE html>

<title>Canal</title>

{% block body %}
    <h1>Canal</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>ExtId</th>
                <td>{{ canal.extId }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ canal.name }}</td>
            </tr>
            <tr>
                <th>IsActiveSw</th>
                <td>{{ canal.isActiveSw ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('canal') }}">{{ 'back_to_list'|trans }}</a>

    <a href="{{ path('canal_edit', {'extId': canal.extId}) }}">edit</a>

    {{ include('canal/_delete_form.html.twig') }}
{% endblock %}
