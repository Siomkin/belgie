{% extends 'layout.html.twig' %}

{% block content -%}
    <h1>Ports</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Portid</th>
                <td>{{ entity.portId }}</td>
            </tr>
            <tr>
                <th>Interface</th>
                <td>{{ entity.interface }}</td>
            </tr>
            <tr>
                <th>Speed</th>
                <td>{{ entity.speed }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ entity.type }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('ports') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('ports_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
